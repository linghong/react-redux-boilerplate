{
  "name": "react-redux-boilerplate",
  "version": "1.0.0",
  "description": "a boilerplate for a simple react-redux site",
  "main": "index.js",
  "scripts": {
    "clean": "rimraf build",
    "serve": "webpack-dev-server",
    "build": "npm run clean SET NODE_ENV=production && webpack -p",
    "deploy": "npm run build && git add build/ git commit -m 'new deploy.' git subtree push --prefix build origin gh-pages"
  },
  "author": "Linghong Chen",
  "license": "ISC",
  "devDependencies": {
    "babel-core": "^6.21.0",
    "babel-loader": "^6.2.10",
    "babel-preset-env": "^1.1.6",
    "babel-preset-es2015": "^6.18.0",
    "babel-preset-react": "^6.16.0",
    "css-loader": "^0.26.1",
    "extract-text-webpack-plugin": "^2.0.0-beta.4",
    "html-webpack-plugin": "^2.26.0",
    "rimraf": "^2.5.4",
    "style-loader": "^0.13.1",
    "webpack": "^2.2.0-rc.0",
    "webpack-dev-server": "^2.2.0-rc.0"
  },
  "dependencies": {
    "image-webpack-loader": "^3.1.0",
    "react": "^15.4.2",
    "react-dom": "^15.4.2",
    "url-loader": "^0.5.7"
  }
}
